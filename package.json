{
  "name": "fastify-webpack-dev-template",
  "version": "0.0.1",
  "description": "A template for develop App server with Fastify & webpack",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "webpack --config webpack.config.js",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?|jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  },
  "keywords": [],
  "author": "bathtimefish",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "fastify": "^3.9.2",
    "fastify-boom": "^1.0.0",
    "fastify-cors": "^5.1.0",
    "fastify-jwt": "^2.4.0",
    "fastify-oauth2": "^4.2.1",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^3.3.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.17",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-node-externals": "^2.5.2"
  }
}
